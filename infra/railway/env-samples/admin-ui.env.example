# Admin UI service
TAILSCALE_AUTHKEY=tskey-admin-ui
TAILSCALE_HOSTNAME=admin-ui

# Auth0 (OIDC login)
AUTH0_DOMAIN=https://dev-example.us.auth0.com
AUTH0_CLIENT_ID=your-admin-ui-client-id
AUTH0_CLIENT_SECRET=your-admin-ui-client-secret
AUTH0_REDIRECT_URI=https://admin-ui.tail1f34d1.ts.net/callback

# Auth0 API audience + M2M credentials used by backend
AUTH0_AUDIENCE=https://identity-broker.tailnet.local/api
AUTH0_M2M_CLIENT_ID=your-m2m-client-id
AUTH0_M2M_CLIENT_SECRET=your-m2m-client-secret
AUTH0_SCOPES=identity:read identity:search tool.access:grant

# Tailnet service URLs
BROKER_URL=http://identity-broker.tail1f34d1.ts.net:8080
TOOL_SERVICE_URL=http://tool-service.tail1f34d1.ts.net:8090

# Session + grant settings
SESSION_SECRET=generate-a-32-byte-random-string
TARGET_GROUP=developers
AGENT_GRANT_PERMISSIONS="toolx:read toolx:write toolx:update"
